<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nineya.springboot.mapper.ResultMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.nineya.springboot.entity.Result">
        <id column="Id" property="id" />
        <result column="counter" property="counter" />
        <result column="crashnum" property="crashnum" />
        <result column="fusionerror" property="fusionerror" />
        <result column="datetime" property="datetime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        Id, counter, crashnum, fusionerror, datetime
    </sql>

</mapper>
